    @Override
    public void paintComponent(java.awt.Graphics g); {
    
    Cirkel liten = new Cirkel();
    liten.x = 110;
    liten.y = 50;
    liten.d = 20;
    
    Cirkel stor = new Cirkel();
    
    stor.x = 95;
    stor.y = 35;
    stor.d = 50;
    
    g.drawOval( liten.x, liten.y, liten.d, liten.d );
    g.drawOval( stor.x, stor.y, stor.d, stor.d );
    
}

